        Title="Change Result" Height="204.224" Width="371.209">
    <Viewbox>
    <Grid Width="359" Margin="0,0,0,0">
        <Label Content="Original" Height="28" HorizontalAlignment="Left" Margin="10,60,0,0" Name="label5" VerticalAlignment="Top" />
