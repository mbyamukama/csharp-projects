<Window x:Class="FRESUgandaERP.DateSelector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Date Selector" Height="189" Width="492" Loaded="Window_Loaded">
    <Grid HorizontalAlignment="Left" Width="482" Margin="2,2,2,2">
        <DatePicker FontSize="14" Name="startDatePicker" HorizontalAlignment="Left" Margin="10,46,0,0" VerticalAlignment="Top" Width="179" SelectedDateChanged="startDatePicker_SelectedDateChanged"/>
        <DatePicker FontSize="14" Name="endDatePicker" HorizontalAlignment="Left" Margin="289,46,0,0" VerticalAlignment="Top" Width="179" SelectedDateChanged="endDatePicker_SelectedDateChanged"/>
        <Label Content="Start Date"   HorizontalAlignment="Left" Margin="12,10,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="14"/>
        <Label Content="End Date"   HorizontalAlignment="Left" Margin="289,12,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="14"/>
        <Button Content="OK" Name="btnOK" HorizontalAlignment="Left" Margin="393,113,0,0" VerticalAlignment="Top" Width="75" Click="btnOK_Click" Height="29"/>

    </Grid>
</Window>
