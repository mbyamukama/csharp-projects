<Window x:Class="FRESUgandaERP.RawDataView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Raw Data View" Height="597.212" Width="1032.195" Loaded="Window_Loaded">
    <Viewbox Margin="0,0,0,0">
        <Grid Margin="0,0,0,0" Height="479">
            <DataGrid AutoGenerateColumns="True" Height="357" HorizontalAlignment="Left" Margin="12,77,0,0" Name="dtGridResults" VerticalAlignment="Top" Width="890" AutoGeneratingColumn="dtGridResults_AutoGeneratingColumn"  BorderBrush="Black" />
            <Button Content="Update" Height="30" HorizontalAlignment="Left" Margin="808,439,0,0" Name="btnUpdate" VerticalAlignment="Top" Width="94" Click="btnUpdate_Click"/>
            <TextBox Height="28" HorizontalAlignment="Right" Margin="0,37,553,0" x:Name="txtSearch" VerticalAlignment="Top" Width="247" BorderBrush="#E6030303" TextChanged="txtSearch_TextChanged"/>
            <Label Content="Search" Height="28" HorizontalAlignment="Left" Margin="12,35,0,0" x:Name="label1" VerticalAlignment="Top"/>
            <ComboBox HorizontalAlignment="Left" Margin="103,6,0,0" VerticalAlignment="Top" Width="140" Name="cbxTableList" SelectionChanged="cbxTableList_SelectionChanged"/>
            <Label Content="Table" Height="28" HorizontalAlignment="Left" Margin="12,6,0,0" x:Name="label1_Copy" VerticalAlignment="Top"/>
        </Grid>
    </Viewbox>
</Window>
