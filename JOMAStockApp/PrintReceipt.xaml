<Window x:Class="StockApp.PrintReceipt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PrintReceipt" Height="177" Width="375">
    <Grid>
        <Label Content="Print Reciept?" FontSize="24" Height="40" HorizontalAlignment="Left" Margin="78,12,0,0" Name="label1" VerticalAlignment="Top" Width="162" />
        <Button Content="YES" FontSize="20" Height="49" HorizontalAlignment="Left" Margin="12,66,0,0" Name="btnYES" VerticalAlignment="Top" Width="77" Click="btnYES_Click" />
        <Button Content="NO" FontSize="20"  Height="49" HorizontalAlignment="Left" Margin="242,66,0,0" Name="btnNO" VerticalAlignment="Top" Width="75" Click="btnNO_Click" />
    </Grid>
</Window>
